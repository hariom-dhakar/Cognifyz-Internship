#include <iostream>

using namespace std;

int main(){
    
    float total = 0.0;
    float marks;
    char grade;
    cout << "Enter student English marks: ";
    cin >> marks;
    if (marks < 0 || marks > 100)
    {
        cout << "Invalid marks. Please enter a marks between 0 and 100." << endl;
        return 1;
    }
    total += marks;
    cout << "Enter student Hindi marks: ";
    cin >> marks;
    if (marks < 0 || marks > 100)
    {
        cout << "Invalid marks. Please enter a marks between 0 and 100." << endl;
        return 1;
    }
    total += marks;
    cout << "Enter student Science marks: ";
    cin >> marks;
    if (marks < 0 || marks > 100)
    {
        cout << "Invalid marks. Please enter a marks between 0 and 100." << endl;
        return 1;
    }
    total += marks;
    cout << "Enter student Maths marks: ";
    cin >> marks;
    if (marks < 0 || marks > 100)
    {
        cout << "Invalid marks. Please enter a marks between 0 and 100." << endl;
        return 1;
    }
    total += marks;
    cout << "Enter student Social Science marks: ";
    cin >> marks;
    if (marks < 0 || marks > 100)
    {
        cout << "Invalid marks. Please enter a marks between 0 and 100." << endl;
        return 1;
    }
    total += marks;
    total = (total / 500) * 100;
    if (total >= 90)
    {
        grade = 'A';
    }
    else if (total >= 80)
    {
        grade = 'B';
    }
    else if (total >= 70)
    {
        grade = 'C';
    }
    else if (total >= 60)
    {
        grade = 'D';
    }
    else
    {
        grade = 'E';
    }
    cout << "Student's grade is: " << grade << endl;

    return 0;
}
